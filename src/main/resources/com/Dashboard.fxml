<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="366.0" prefWidth="522.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.finalproj.Controller.DashboardController">
    <Label layoutX="96.0" layoutY="24.0" prefHeight="68.0" prefWidth="330.0" text="Chào Mừng Đến Với Trang Quản Lý Cửa Hàng Điện Thoại Di Động" textAlignment="CENTER" textFill="#5473c9" wrapText="true">
      <font>
         <Font name="System Bold" size="20.0" />
      </font></Label>
    <Button fx:id="manageCustomerBtn" layoutX="200.0" layoutY="121.0" onAction="#handleManageCustomer" prefHeight="26.0" prefWidth="122.0" text="Quản lý khách hàng" />
    <Button fx:id="manageProductBtn" layoutX="200.0" layoutY="171.0" onAction="#handleManageProduct" prefHeight="26.0" prefWidth="122.0" text="Quản lý sản phẩm" />
    <Button fx:id="manageInvoiceBtn" layoutX="200.0" layoutY="225.0" onAction="#handleManageInvoice" prefHeight="26.0" prefWidth="122.0" text="Quản lý hóa đơn" />
    <Button fx:id="logoutBtn" layoutX="200.0" layoutY="281.0" onAction="#handleLogout" prefHeight="26.0" prefWidth="122.0" text="Đăng xuất" />
</AnchorPane>
